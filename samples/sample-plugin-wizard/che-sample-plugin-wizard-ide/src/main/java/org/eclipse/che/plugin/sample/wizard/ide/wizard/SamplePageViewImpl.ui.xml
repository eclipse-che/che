<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style >
        .mainDoc {
            margin: 15px;
        }

        .mainDoc div {
            overflow: visible !important;
        }
    </ui:style>
    <g:DockLayoutPanel width="100%" height="100%" addStyleNames="{style.mainDoc}">
        <g:north size="100">
            <g:FlowPanel>
                <g:Label text="Compiler version:" />
                <g:TextBox width="500px" ui:field="compilerVersion"/>
            </g:FlowPanel>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>
