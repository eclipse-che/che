{
    "name": "theia-cypress",
    "version": "0.0.0",
    "description": "E2E tests for Theia",
    "main": "index.js",
    "engines": {
        "node": ">=4.0.0"
    },
    "scripts": {
        "cypress:version": "cypress version",
        "cypress:verify": "cypress verify",
        "cypress:open": "cypress open --global",
        "cypress:run": "node ./scripts/run.js",
        "cypress:run:record": "node ./scripts/run.js --record",
        "cypress:run:chrome": "node ./scripts/run.js --browser chrome",
        "cypress:run:chrome:record": "node ./scripts/run.js --browser chrome --record",
        "test:ci": "run-p --race start cypress:run",
        "test:ci:record": "run-p --race start cypress:run:record",
        "test:ci:chrome": "run-p --race start cypress:run:chrome",
        "test:ci:chrome:record": "run-p --race start cypress:run:chrome:record",
        "start": "node ./scripts/start.js",
        "dev": "nodemon ./scripts/start.js --watch examples",
        "stop-only": "stop-only examples",
        "lint:js": "eslint --fix scripts/*.js"
    },
    "author": "Florent Benoit",
    "license": "EPL-2.0",
    "dependencies": {
        "body-parser": "1.18.3",
        "chai-colors": "1.0.1",
        "chai-date-string": "0.1.0",
        "chai-enzyme": "1.0.0-beta.1",
        "chalk": "2.4.1",
        "csurf": "1.9.0",
        "debug": "4.1.0",
        "dragula": "3.7.2",
        "enzyme": "3.8.0",
        "enzyme-adapter-react-16": "1.7.1",
        "execa": "1.0.0",
        "express": "4.16.4",
        "express-session": "1.15.6",
        "hbs": "4.0.1",
        "http-server": "0.11.1",
        "jquery": "3.3.1",
        "json-server": "0.14.0",
        "lodash": "4.17.11",
        "minimist": "1.2.0",
        "morgan": "1.9.1",
        "react": "16.7.0",
        "react-dom": "16.7.0"
    },
    "devDependencies": {
        "@cypress/snapshot": "2.0.1",
        "@cypress/webpack-preprocessor": "4.0.2",
        "@types/chai": "4.1.7",
        "@types/mocha": "5.2.5",
        "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
        "bluebird": "3.5.3",
        "cypress": "3.1.3",
        "eslint": "5.10.0",
        "eslint-plugin-cypress-dev": "2.0.0",
        "eslint-plugin-mocha": "5.2.0",
        "fs-extra": "7.0.1",
        "glob": "7.1.3",
        "json-server-reset": "1.0.0",
        "nodemon": "1.18.9",
        "npm-run-all": "4.1.5",
        "pretty-ms": "4.0.0",
        "stop-only": "2.2.5",
        "ts-loader": "5.3.2",
        "tsify": "4.0.1",
        "tslint": "5.12.0",
        "typescript": "3.2.2",
        "webpack": "4.28.1"
    }
}

